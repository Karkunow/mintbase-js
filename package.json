{
  "name": "mintbase",
  "version": "0.0.5",
  "description": "Mint, sell, transfer NFTs on the NEAR blockchain",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "test": "jest",
    "dist": "parcel build browser-exports.js -d dist/ --no-source-maps --out-file mintbase.js",
    "lint": "eslint src/**/*.ts",
    "fix": "eslint src/**/*.ts --fix",
    "prepublish": "npm run lint && npm run build && npm run dist && npm run doc",
    "doc": "typedoc src",
    "start:docs": "http-server ./docs",
    "start:example": "npm run build && rm -f ./examples/mintbase.js && npm run dist && cp ./dist/mintbase.js ./examples/mintbase.js && http-server ./examples/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mintbase/mintbase-js.git"
  },
  "files": [
    "lib",
    "dist"
  ],
  "keywords": [
    "NFT",
    "Blockchain",
    "Mintbase"
  ],
  "author": "@nategeier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mintbase/mintbase-js/issues"
  },
  "homepage": "https://github.com/Mintbase/mintbase-js#readme",
  "dependencies": {
    "bn.js": "^5.2.0",
    "browser-or-node": "^1.3.0",
    "crypto": "^1.0.1",
    "firebase": "^8.3.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "isomorphic-unfetch": "^3.1.0",
    "near-api-js": "^0.39.0",
    "regenerator-runtime": "^0.13.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/browser-or-node": "^1.3.0",
    "@types/jest": "^26.0.22",
    "@types/node-fetch": "^2.5.8",
    "@types/regenerator-runtime": "^0.13.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-eslint": "^10.1.0",
    "browserify": "^17.0.0",
    "eslint": "^7.22.0",
    "esmify": "^2.1.1",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.5",
    "tinyify": "^3.0.0",
    "ts-jest": "^26.5.3",
    "tsc": "^1.20150623.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.20.33",
    "typescript": "^4.2.3",
    "uglifyify": "^5.0.2"
  }
}
