{
  "name": "mintbase",
  "version": "0.0.5",
  "description": "Mint, sell, transfer NFTs on the NEAR blockchain",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "test": "jest",
    "dist": "npm run browserify",
    "start:example": "npm run build && rm ./examples/mintbase.js && npm run dist && http-server ./examples/",
    "browserify": "browserify browser-exports.js -o dist/mintbase.js && browserify browser-exports.js -o examples/mintbase.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mintbase/mintbase-js.git"
  },
  "keywords": [
    "NFT",
    "Blockchain",
    "Mintbase"
  ],
  "author": "@nategeier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mintbase/mintbase-js/issues"
  },
  "homepage": "https://github.com/Mintbase/mintbase-js#readme",
  "dependencies": {
    "browser-or-node": "^1.3.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "isomorphic-unfetch": "^3.1.0",
    "near-api-js": "^0.38.0"
  },
  "devDependencies": {
    "@types/browser-or-node": "^1.3.0",
    "@types/chai": "^4.2.15",
    "@types/jest": "^26.0.20",
    "@types/node-fetch": "^2.5.8",
    "browserify": "^17.0.0",
    "chai": "^4.3.3",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "mocha-typescript": "^1.1.17",
    "ts-jest": "^26.5.3",
    "tsc": "^1.20150623.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  }
}
